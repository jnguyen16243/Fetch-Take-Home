{
    "version": 2,
    "builds": [
      {
        "src": "fetch-dog-app/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      },
      {
        "src": "fetch-dog-backend/src/server.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "fetch-dog-backend/src/server.ts"
      },
      { "src": "/[^.]+", "dest": "/", "status": 200 }
    ]
  }
  
  